      <div class="emp_profile_details" >{{data.company_site}}</div> 
    <h5><a id="skills_wanted">Skills We're Looking For:</a></h5>
      <div class="emp_profile_details" >{{data.skills_wanted}}</div> 
    <h5><a id="company_departments">Company Department:</a></h5>
      <div class="emp_profile_details" >{{data.company_departments}}</div>
    <h5><a id="company_industry">Company Industry:</a></h5>
      <div class="emp_profile_details">{{data.company_industry}}</div>
    <h5><a id="min_years">Minimum Years of Experience Required:</a></h5>
      <div class="emp_profile_details" >{{data.min_years}}</div>
  </div>

<script src = "../js/introHCI.js" ></script>